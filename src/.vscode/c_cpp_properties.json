{
    "env": {
      "myDefaultIncludePath": ["${workspaceFolder}", "${workspaceFolder}/include","${workspaceFolder}"],
      "myCompilerPath": "/usr/local/bin/gcc-7",
      "cpp_bindings_path":"../godot-cpp/",
      "godot_headers_path": "../godot-cpp/godot_headers/",
      "cpp_library" : "libgodot-cpp"
    },
    "configurations": [
      {
        "name": "MingGW",
        "intelliSenseMode": "clang-x64",
        "includePath": ["${myDefaultIncludePath}", "${cpp_bindings_path}/include","${cpp_bindings_path}/include/core","${cpp_bindings_path}/include/gen/","${godot_headers_path}"],
        "defines": ["FOO", "BAR=100"],
        "forcedInclude": ["${workspaceFolder}/include/config.h"],
        "compilerPath": "/usr/bin/clang",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "browse": {
          "path": ["${workspaceFolder}"],
          "limitSymbolsToIncludedHeaders": true,
          "databaseFilename": ""
        }
      }
    ],
    "version": 4
  }